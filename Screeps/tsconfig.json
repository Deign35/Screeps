{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "outDir": "obj/",
        "baseUrl": "src/",
        "sourceMap": true,
        "strict": true,
        "experimentalDecorators": true,
        "noImplicitReturns": true,
        "allowSyntheticDefaultImports": true,
        "allowUnreachableCode": false
    },
    "exclude": [
        "node_modules",
        "objs",
        "dist"
    ]
}